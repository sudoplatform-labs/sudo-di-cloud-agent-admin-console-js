{
  "name": "@sudoplatform-labs/sudo-di-cloud-agent-admin-console",
  "description": "Sudo Decentalized Identity Cloud Agent Admin Console Application",
  "version": "0.4.0",
  "author": "Anonyome Platform Team",
  "license": "Apache-2.0",
  "files": [
    "build"
  ],
  "scripts": {
    "copy-acapy-config": "cp -f node_modules/@sudoplatform-labs/sudo-di-cloud-agent/bin/acapy.json ./public/",
    "add-acapy-seed": "./utils/add-acapy-seed.sh -c ./public/acapy.json",
    "bootstrap": "yarn install && yarn copy-acapy-config",
    "start": "react-scripts start",
    "lint": "eslint ./src --ext ts",
    "lint:prettier": "prettier -l \"src/**/*.{js,json,jsx,ts,tsx,}\"",
    "write:prettier": "prettier --write \"src/**/*.{js,json,jsx,ts,tsx,}\"",
    "build": "react-scripts build",
    "build:prod": "yarn audit && yarn build",
    "build:ts-check": "yarn tsc --noEmit",
    "test:update-acapy-address": "./utils/update-acapy-address.sh -c ./build/acapy.json -h cloud-agent",
    "test:execute-e2e": "jest --runInBand -c jest.e2e.config.js",
    "test:up-browser-server-debug": "./e2e/browser-server.sh up -i selenium/standalone-chrome-debug:3.141.59 -p 4445",
    "test:up-browser-server": "./e2e/browser-server.sh up -i selenium/standalone-chrome:3.141.59 -p 4445",
    "test:down-browser-server": "./e2e/browser-server.sh down",
    "test:up-e2e-env": "yarn di-env up -c $PWD/public/acapy.json && yarn test:update-acapy-address && yarn test:up-browser-server-debug",
    "test:down-e2e-env": "yarn test:down-browser-server; yarn di-env down",
    "test:run-e2e": "rm -rf .e2e_test_success && yarn test:up-e2e-env && yarn test:execute-e2e && touch .e2e_test_success; yarn test:down-e2e-env",
    "test:unit": "yarn react-scripts test",
    "test:e2e": "yarn test:run-e2e",
    "test:all": "CI=true yarn test:unit && yarn test:e2e ",
    "coverage:clean": "cd ./instrumented && rm -rf ./e2e ./src ./.nyc_output ./public ./utils ./devel",
    "coverage:create-instrumented-dir": "mkdir instrumented && tar -cf - node_modules | tar -xf - -C ./instrumented",
    "coverage:refresh-instrumented-dir": "yarn coverage:clean && tar -cf - --exclude instrumented --exclude build --exclude .git --exclude .nyc_output --exclude coverage-e2e --exclude node_modules . | tar -xf - -C ./instrumented",
    "coverage:instrument": "cd ./instrumented && nyc instrument --in-place --nycrc-path='./nyc.e2e.config.json' ./src && ./utils/add-lint-disables.sh ./src && react-scripts build",
    "coverage:build": "test -d ./instrumented || yarn coverage:create-instrumented-dir && yarn coverage:refresh-instrumented-dir && yarn coverage:instrument && yarn coverage:refresh-instrumented-dir",
    "coverage:copy-unit-test-coverage": "cd ./instrumented && mkdir .nyc_output; cp -f coverage/coverage-final.json .nyc_output/out.json",
    "coverage:unit-tests": "cd ./instrumented && CI=true yarn test:unit --coverage",
    "coverage:e2e-tests": "cd ./instrumented && yarn test:run-e2e",
    "coverage:all-tests": "yarn coverage:unit-tests; yarn coverage:copy-unit-test-coverage; yarn coverage:e2e-tests",
    "coverage:report": "rm -rf ./.nyc_output && cp -R instrumented/.nyc_output ./ && nyc report --nycrc-path='./nyc.e2e.config.json'",
    "coverage": "rm -rf ./instrumented; yarn coverage:build && yarn coverage:all-tests; yarn coverage:report"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.5",
    "@sudoplatform-labs/sudo-di-cloud-agent": "^0.6.1",
    "antd": "4.9.4",
    "antd-mask-input": "^0.1.13",
    "color": "^3.2.0",
    "d3": "^5.12.0",
    "https-proxy-agent": "^5.0.0",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.2",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "portable-fetch": "^3.0.0",
    "qrcode.react": "^1.0.0",
    "react": "^16.9.1",
    "react-dom": "^16.9.1",
    "react-use": "^14.0.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-syntax-typescript": "^7.8.3",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.12.7",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/color": "^3.0.1",
    "@types/d3": "^5.7.2",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.17.9",
    "@types/jest": "^25.1.5",
    "@types/jest-image-snapshot": "^4.1.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.7",
    "@types/numeral": "^0.0.26",
    "@types/pretty": "^2.0.0",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/selenium-webdriver": "^4.0.10",
    "@types/styled-components": "^5.0.1",
    "@types/webpack": "^4.41.0",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-preset-jest": "^26.6.2",
    "chromedriver": "^89.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^3.0.0",
    "express": "^4.17.1",
    "geckodriver": "^1.21.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-style-chardy": "^1.0.0",
    "jest-image-snapshot": "^4.2.0",
    "jest-styled-components": "^7.0.0",
    "moment-timezone": "^0.5.28",
    "node-fetch": "^2.6.1",
    "nyc": "^15.1.0",
    "nyc-config-tsx": "^0.1.0",
    "prettier": "^2.0.2",
    "pretty": "^2.0.0",
    "react-is": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "react-test-renderer": "^16.14.0",
    "selenium-webdriver": "^4.0.0-alpha.8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~4.4.3"
  },
  "resolutions": {
    "**/node-fetch": "^2.6.1",
    "**/yargs-parser": "^13.1.2",
    "**/axios": "^0.21.1",
    "**/lodash": "^4.17.21",
    "**/hosted-git-info": "^2.8.9",
    "**/url-parse": "^1.5.0",
    "**/path-parse": "^1.0.7",
    "react-scripts/webpack/terser-webpack-plugin/cacache/ssri": "^6.0.2",
    "react-scripts/terser-webpack-plugin/cacache/ssri": "^8.0.1",
    "react-scripts/react-dev-utils/immer": "^8.0.1",
    "**/elliptic": "^6.5.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!src/test/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
